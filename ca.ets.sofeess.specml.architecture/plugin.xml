<plugin>
   <extension
         point="org.eclipse.papyrus.infra.architecture.models">
      <model
            path="resources/specml.architecture">
      </model>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings">
      <clientContext
            id="ca.ets.sofeess.specml.architecture">
         <matcher
               class="ca.ets.sofeess.specml.architecture.internal.NoContextEnablementMatcher">
         </matcher>
      </clientContext>
      <binding
            context="ca.ets.sofeess.specml.architecture">
         <elementType
               ref="org.eclipse.gmf.runtime.diagram.core.advice.notationDepdendents">
         </elementType>
         <elementType
               ref="org.eclipse.gmf.runtime.diagram.core.advice.removeBookmarks">
         </elementType>
         <advice
               ref="org.eclipse.papyrus.infra.emf.readOnlyAdvice">
         </advice>
      </binding>
   </extension>
</plugin>
