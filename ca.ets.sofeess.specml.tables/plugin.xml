<plugin>
   <extension
         point="org.eclipse.papyrus.infra.nattable.configuration">
      <configuration
            file="resources/srats.nattableconfiguration"
            type="SpecSRATSTable">
      </configuration>
      <configuration
            file="resources/hlrs.nattableconfiguration"
            type="SpecHLRsTable">
      </configuration>
      <configuration
            file="resources/pbs.nattableconfiguration"
            type="SpecPBSsTable">
      </configuration>
   </extension>
   <extension
         point="org.eclipse.papyrus.infra.nattable.tester">
      <tester
            class="ca.ets.sofeess.specml.tables.SRATSTableTester"
            id="ca.ets.sofeess.specml.tables.canCreateSRATSTable">
      </tester>
      <tester
            class="ca.ets.sofeess.specml.tables.HLRsTableTester"
            id="ca.ets.sofeess.specml.tables.canCreateHLRsTable">
      </tester>
      <tester
            class="ca.ets.sofeess.specml.tables.PBSsTableTester"
            id="ca.ets.sofeess.specml.tables.canCreatePBSsTable">
      </tester>
   </extension>
   <extension
         point="org.eclipse.papyrus.infra.nattable.axismanager">
      <axisManager
            id="ca.ets.sofeess.specml.tables.srats.axisManager"
            manager="ca.ets.sofeess.specml.tables.SRATSAxisManager">
      </axisManager>
      <axisManager
            id="ca.ets.sofeess.specml.tables.hlrs.axisManager"
            manager="ca.ets.sofeess.specml.tables.HLRsAxisManager">
      </axisManager>
      <axisManager
            id="ca.ets.sofeess.specml.tables.pbs.axisManager"
            manager="ca.ets.sofeess.specml.tables.PBSsAxisManager">
      </axisManager>
   </extension>

</plugin>
