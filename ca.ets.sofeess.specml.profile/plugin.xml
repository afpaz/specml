<plugin>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated specml.profile -->
      <package
            uri="http://www.etsmtl.ca/sofeess/specml"
            class="ca.ets.sofeess.specml.SpecMLPackage"
            genModel="model/specml.profile.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="pathmap://SPECML_PROFILE/"
            target="platform:/plugin/ca.ets.sofeess.specml.profile/model/">
      </mapping>
   </extension>
   <extension
         point="org.eclipse.uml2.uml.generated_package">
      <profile
            location="pathmap://SPECML_PROFILE/specml.profile.uml#_EohtkHMyEeiGk6p11I42Qw"
            uri="http://www.etsmtl.ca/sofeess/specml">
      </profile>
   </extension>
   <extension
         point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
      <profile
            description="UML profile for requirements specification"
            iconpath="icons/specml.gif"
            name="SpecML"
            path="pathmap://SPECML_PROFILE/specml.profile.uml"
            provider="École de Technologie Supérieure">
      </profile>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.factory_override">
      <factory
            class="ca.ets.sofeess.specml.customimpl.SpecFactoryCustomImpl"
            uri="http://www.etsmtl.ca/sofeess/specml">
      </factory>
   </extension>
</plugin>
